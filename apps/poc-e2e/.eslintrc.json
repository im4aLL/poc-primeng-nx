{
    "extends": ["plugin:cypress/recommended", "../../.eslintrc.json"],
    "ignorePatterns": ["!**/*"],
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
            "rules": {
                "no-undef": "off",
                "require-jsdoc": "off",
                "@typescript-eslint/explicit-function-return-type": "off",
                "@typescript-eslint/naming-convention": "off",
                "no-console": "off"
            }
        }
    ]
}
